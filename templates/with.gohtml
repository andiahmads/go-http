<html>

    <head>
        <title>
            {{.Title}}
        </title>
    </head>

    <body>
    <h1>Name: {{.Name}}</h1> 
    {{with .Address}} 
        <h1>Address street: {{.Street}}</h1> 
        <h1>Address city: {{.City}}</h1> 
     {{else}}
        <h1>Address street: null</h1> 
        <h1>Address city: null</h1> 

    {{end}}    
    </body>

</html>